{
  "#": "NLP.js chat-bot configuration.",

  "pathPipeline": "./bot/pipelines.md",

  "pathPlugins": "./src/plugins",

  "settings": {
    "nlp": {
      "corpora": [
        "./bot/corpus-en.json"
      ]
    },

    "console": {
      "debug": true
    },

    "api-server": {
      "clientPath": "./public",
      "port": 3000,
      "serveBot": true
    },

    "surveyBot": {
      "surveys": [
        "./bot/survey-en.json"
      ]
    },

    "msbf": { }
  },

  "nlpjs": { },

  "verbose": false,

  "use": [ "Basic", "ConsoleConnector", "ExpressApiServer", "DirectlineConnector" ],

  "X_not_used": [ "MsbfConnector" ]
}
